@{ ViewBag.Title = "About";
}
<div class="jumbotron">
    <h2>@ViewBag.Title</h2>
    <h3>General Inquiries:</h3>
    gccoffee@grandcircus.com
    <p>

        <h4>Hours:</h4>
        Tues-Fri  7:30am to 6pm<br />
        Sat  8:30am to 6pm<br />
        Sun  9am to 5pm<br />
    </p>

    @*<p>You have visited this page @Response.Cookies["Counter"].Value times.</p>*@

    <div id="map"></div>
    <script>
        function initMap() {
            var uluru = { lat: 42.3359526, lng: -83.0497719 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 13,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyANKULz-kqwjLnC1v6RaoRFfifNpqEcLBI&callback=initMap">
    </script>
</div>
<style>
    #map {
        margin: 0 auto;
        width: 100%;
        height: 200px;
        background-color: grey;
    }
</style>
